# ✅ CONFIGURACIÓN HACKATHON - The AI Championship 2025
# IMPORTANTE: Copia este archivo a .env y rellena con tus propias claves

# Google Gemini API
VITE_GEMINI_API_KEY=tu_api_key_aqui
REACT_APP_GOOGLE_API_KEY=tu_api_key_aqui

# Vultr API (opcional para deployment)
VULTR_API_TOKEN=tu_vultr_token_aqui
VULTR_SERVER_IP=tu_servidor_ip
VITE_VULTR_PROXY_URL=http://tu_servidor:3001

# Raindrop/LiquidMetal API (opcional)
RAINDROP_API_KEY=tu_raindrop_key
VITE_RAINDROP_API_KEY=tu_raindrop_key

# Puerto del proxy (opcional)
PROXY_PORT=3002

# Configuración del entorno  
NODE_ENV=development
REACT_APP_DEMO_MODE=true

# ⚠️ SEGURIDAD: Nunca subas el archivo .env al repositorio
# Las claves API son secretas y personales
